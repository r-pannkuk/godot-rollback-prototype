config_version=5

[application]

config/name="RollbackPrototype"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

SyncManager="*res://addons/godot-rollback-netcode/SyncManager.gd"
SyncDebugger="*res://addons/godot-rollback-netcode/SyncDebugger.gd"
SyncReplay="*res://addons/godot-rollback-netcode/SyncReplay.gd"
GlobalRNG="*res://addons/godot-rollback-netcode/NetworkRandomNumberGenerator.gd"

[dotnet]

project/assembly_name="RollbackPrototype"

[editor_plugins]

enabled=PackedStringArray("res://addons/external_editor/plugin.cfg", "res://addons/godot-rollback-netcode/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "green",
"res://dist/": "gray",
"res://scenes/": "orange",
"res://scripts/": "purple"
}

